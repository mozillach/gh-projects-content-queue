{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "plugins": [
        "ava"
    ],
    "rules": {
        "node/no-unsupported-features": 0
    },
    "settings": {
        "import/resolver": "node"
    },
    "extends": [
        "plugin:ava/recommended",
        "plugin:import/errors"
    ]
}
